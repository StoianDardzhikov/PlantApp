@page
@model RegisterModel
@{
    ViewData["Title"] = "Register";
}


<div class="login-register-page-container">
    <div class="form-container">
        <h1 class="login-register-title">Регистрирай се</h1>
        <form asp-route-returnUrl="@Model.ReturnUrl" method="post">
            <div asp-validation-summary="All" class="text-danger"></div>
            <div class="input-container">
                <label class="login-register-label">Е-поща</label>
                <input asp-for="Input.Email" class="login-register-input" />
                <span asp-validation-for="Input.Email" class="text-danger"></span>
            </div>
            <div class="input-container">
                <label class="login-register-label">Парола</label>
                <input asp-for="Input.Password" class="login-register-input" />
                <span asp-validation-for="Input.Password" class="text-danger"></span>
            </div>
            <div class="input-container">
                <label class="login-register-label">Потвърди парола</label>
                <input asp-for="Input.ConfirmPassword" class="login-register-input" />
                <span asp-validation-for="Input.ConfirmPassword" class="text-danger"></span>
            </div>
            <div class="login-register-submit-container">
                <button type="submit" class="btn-login-register-submit">
