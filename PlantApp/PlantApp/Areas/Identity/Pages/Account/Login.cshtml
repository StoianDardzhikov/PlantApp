@page
@model LoginModel

@{
    ViewData["Title"] = "Log in";
}

<div class="login-register-page-container">
    <div class="form-container">
        <h1 class="login-register-title">Влезни</h1>
        <form id="account" method="post">
            <div asp-validation-summary="All" class="text-danger"></div>
            <div class="input-container">
                <label class="login-register-label" asp-for="Input.Email">Е-поща</label>
                <input asp-for="Input.Email" class="login-register-input" />
                <span asp-validation-for="Input.Email" class="text-danger"></span>
            </div>
            <div class="input-container">
                <label class="login-register-label" asp-for="Input.Password">Парола</label>
                <input asp-for="Input.Password" class="login-register-input" />
                <span asp-validation-for="Input.Password" class="text-danger"></span>
            </div>
            <div class="login-register-submit-container">
                <button type="submit" class="btn-login-register-submit">Влезни</button>
                <p class="login-register-link">Нямаш акаунт? <a asp-area="Identity" asp-page="/Account/Register">Регистрирай се</a> сега!</p>
            </div>
        </form>
    </div>
